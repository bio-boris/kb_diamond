      "field_type": "text",
      "text_options": {
        "validate_as": "float",
        "min_integer": 25
      }
    },
    {
      "id": "ident_thresh",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        "97.0"
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float",
        "min_float": 0.0,
        "max_float": 100.0
      }
    },
    {
      "id": "overlap_fraction",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        "50.0"
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float",
        "min_float": 0.0,
        "max_float": 100.0
      }
    },
    {
      "id": "maxaccepts",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        "1000"
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "int",
        "min_integer": 1
      }
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "kb_diamond",
